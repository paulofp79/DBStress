{
  "name": "dbstress",
  "version": "1.0.0",
  "description": "Oracle Database Stress Testing Tool with Online Sales Schema",
  "main": "server/index.js",
  "scripts": {
    "server": "node server/index.js",
    "client": "cd client && npm start",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "install-all": "npm install && cd client && npm install",
    "build": "cd client && npm run build",
    "start": "npm run build && node server/index.js",
    "serve": "node server/index.js"
  },
  "keywords": [
    "oracle",
    "database",
    "stress-test",
    "benchmark"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "oracledb": "^6.2.0",
    "socket.io": "^4.7.2",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
